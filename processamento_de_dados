#2

Algoritmo "area"

Var 

  b1, b2, h, area : real
   
   

Inicio

  b1 <-6.0 
  b2 <- 8.0
  h  <- 5.0

 area <- (b1 + b2) / 2.0 * h
  escreval(area)
  
Fimalgoritmo
